
{
  "name": "teamplan360",
  "version": "1.0.0",
  "description": "Team work planning and reporting system with AI-powered insights",
  "main": "backend/server.js",
  "scripts": {
    "dev": "concurrently \"npm run dev:backend\" \"npm run dev:frontend\"",
    "dev:backend": "cd backend && npm run dev",
