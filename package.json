    "build": "cd frontend && npm run build",
    "start": "cd backend && npm start"
  },
  "keywords": ["team", "planning", "reporting", "ai", "deepseek"],
  "author": "TeamPlan360",
  "license": "MIT",
  "devDependencies": {
    "concurrently": "^8.2.2"
  }
}
